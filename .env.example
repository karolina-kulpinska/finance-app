# Skopiuj ten plik jako .env i uzupełnij wartości. Plik .env nie jest commitowany.

# Firebase (wartości z konsoli Firebase → Ustawienia projektu)
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_MEASUREMENT_ID=

# Price ID z Stripe (Krok 2 w docs/ROZSZERZENIE_STRIPE_KROK_PO_KROKU.md)
# Np. price_1ABC... dla produktu „Plan Pro”
REACT_APP_STRIPE_PRICE_ID=

# Adres strony (dla linków zaproszeniowych i e-maili). Na localhost używany jest origin.
# Np. https://smartbudget.pl lub https://www.smartbudget.pl
REACT_APP_SITE_URL=

# Email właściciela aplikacji – widoczny przycisk "Przyznaj sobie Pro" (bez płatności). Opcjonalne.
# Np. kulpinska.karolinaa@gmail.com
REACT_APP_ADMIN_EMAIL=
